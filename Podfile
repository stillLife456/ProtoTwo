pod 'LTMorphingLabel'
pod 'Charts', '2.3.0'

  target 'ProtoTwoTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ProtoTwoUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

